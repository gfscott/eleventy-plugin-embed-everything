{
  "name": "eleventy-plugin-embed-everything",
  "version": "1.10.0",
  "description": "An Eleventy plugin that allows you to quickly add common web embeds to markdown posts, using only their URLs",
  "main": ".eleventy.js",
  "scripts": {
    "coverage": "coverage",
    "test": "nyc --reporter=lcov ava",
    "lint": "npx rome check"
  },
  "ava": {
    "files": [
      "!test/inc"
    ]
  },
  "keywords": [
    "11ty",
    "eleventy",
    "eleventy-plugin",
    "embeds",
    "embedding",
    "instagram",
    "media",
    "soundcloud",
    "spotify",
    "tiktok",
    "twitch",
    "twitter",
    "youtube",
    "vimeo"
  ],
  "license": "MIT",
  "dependencies": {
    "deepmerge": "^4.2.2",
    "eleventy-plugin-embed-instagram": "^1.2.0",
    "eleventy-plugin-embed-soundcloud": "^1.2.0",
    "eleventy-plugin-embed-spotify": "^1.2.0",
    "eleventy-plugin-embed-tiktok": "^1.1.0",
    "eleventy-plugin-embed-twitch": "^1.2.0",
    "eleventy-plugin-embed-twitter": "^1.1.0",
    "eleventy-plugin-vimeo-embed": "^1.3.0",
    "eleventy-plugin-youtube-embed": "^1.5.0"
  },
  "author": {
    "name": "Graham F. Scott",
    "email": "gfscott@gmail.com",
    "url": "https://gfscott.com"
  },
  "homepage": "https://gfscott.com/embed-everything/",
  "repository": {
    "type": "git",
    "url": "https://github.com/gfscott/eleventy-plugin-embed-everything.git"
  },
  "bugs": "https://github.com/gfscott/eleventy-plugin-embed-everything/issues",
  "devDependencies": {
    "ava": "^3.15.0",
    "codecov": "^3.8.1",
    "nyc": "^15.1.0",
    "rome": "^10.0.4-beta"
  }
}
